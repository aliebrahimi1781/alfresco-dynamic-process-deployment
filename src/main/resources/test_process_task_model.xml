<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="ks:taskModel">
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/bpm/1.0" prefix="bpm" />
	</imports>
	<namespaces>
		<namespace uri="http://www.alfresco.org/model/kickstart/1.0" prefix="ks" />
	</namespaces>
	<types>
		<type name="ks:a">
			<parent>bpm:startTask</parent>
			<properties>
				<property name="ks:field_a">
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="ks:field_b">
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</type>
		<type name="ks:b">
			<parent>bpm:startTask</parent>
			<properties>
				<property name="ks:field_c">
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</type>
	</types>
</model>

